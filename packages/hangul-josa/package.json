{
  "name": "hangul-josa",
  "version": "0.1.0",
  "description": "Let's handle 은/는, 을/를, 이/가, 와/과 with ease using modern javascript syntax!",
  "keywords": [
    "Hangul",
    "Korean",
    "josa",
    "postposition"
  ],
  "homepage": "https://github.com/simnalamburt/hangul-josa",
  "bugs": {
    "url": "https://github.com/simnalamburt/hangul-josa/issues"
  },
  "license": "(APACHE-2.0 OR MIT)",
  "author": "Hyeon Kim <simnalamburt@gmail.com> (https://hyeon.me)",
  "repository": {
    "type": "git",
    "url": "https://github.com/simnalamburt/hangul-josa.git"
  },
  "devDependencies": {
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.0",
    "microbundle": "^0.12.3",
    "mocha": "^8.1.1",
    "sucrase": "^3.15.0",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "build": "microbundle -f cjs,modern,umd",
    "dev": "microbundle -f cjs,modern,umd watch",
    "test": "tsc && mocha"
  },
  "source": "src/index.ts",
  "main": "dist/hangul-josa.js",
  "module": "dist/hangul-josa.modern.js",
  "unpkg": "dist/hangul-josa.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "sideEffects": false,
  "mocha": {
    "extension": [
      "ts"
    ],
    "require": "sucrase/register"
  }
}
